    "react-snap": "^1.23.0",
    "sharp": "^0.34.2",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.2.9",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
