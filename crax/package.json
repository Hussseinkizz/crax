{
  "name": "crax",
  "description": "Create React App Xtended",
  "author": "Hussein Kizz",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "start": "pnpm generate:icons && vite --port 3000",
    "build": "vite build",
    "generate:sitemap": "tsx .crax/scripts/generate-sitemap.mjs",
    "generate:ssg": "react-snap --config .crax/react-snap.config.mjs",
    "generate:icons": "tsx .crax/scripts/generate-icons.mjs",
    "package": "vite build && pnpm generate:sitemap && pnpm generate:ssg && pnpm generate:icons",
    "clean": "rm -rf dist public/sitemap.xml public/robots.txt .crax/icons.generated.json",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@regist/regist": "^0.0.3",
    "@tailwindcss/vite": "^4.1.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "js.foresight": "^3.0.1",
    "lucide-react": "^0.509.0",
    "next-themes": "^0.4.6",
    "pwa-asset-generator": "^8.0.5",
    "radix-ui": "latest",
    "react": "^19.1.0",
    "react-day-picker": "8.10.1",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.3",
    "react-resizable-panels": "^3.0.1",
    "react-router": "^7.6.3",
    "react-router-dom": "^7.6.3",
    "recharts": "^2.15.3",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.6",
    "vaul": "^1.1.2",
    "vite-imagetools": "^7.1.0",
    "vite-plugin-mdx": "^3.6.1",
    "vite-plugin-pages": "^0.33.1",
    "vite-plugin-pwa": "^1.0.1",
    "vite-ssg": "^28.0.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
